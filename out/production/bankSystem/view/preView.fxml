   <children>
      <Label layoutX="150.0" layoutY="28.0" text="Welcome to banking system">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="227.0" layoutY="103.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="79.0" prefWidth="146.0" text="LOGIN" />
      <Button fx:id="registerButton" layoutX="227.0" layoutY="223.0" mnemonicParsing="false" onAction="#onRegister" prefHeight="79.0" prefWidth="146.0" text="REGISTER" />
   </children>
</AnchorPane>
